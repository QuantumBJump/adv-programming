<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zoo.AddPenController">
   <children>
      <Label layoutX="20.0" layoutY="25.0" text="Type:" />
      <RadioButton fx:id="dryTypeRadioButton" layoutX="71.0" layoutY="24.0" mnemonicParsing="false" onAction="#typeSetDry" text="Dry">
         <toggleGroup>
            <ToggleGroup fx:id="typeToggleGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="semiwetTypeRadioButton" layoutX="132.0" layoutY="24.0" mnemonicParsing="false" onAction="#typeSetSemiwet" text="Semiwet" toggleGroup="$typeToggleGroup" />
      <RadioButton fx:id="wetTypeRadioButton" layoutX="222.0" layoutY="24.0" mnemonicParsing="false" onAction="#typeSetWet" text="Aquarium" toggleGroup="$typeToggleGroup" />
      <RadioButton fx:id="airTypeRadioButton" layoutX="327.0" layoutY="24.0" mnemonicParsing="false" onAction="#typeSetAir" text="Aviary" toggleGroup="$typeToggleGroup" />
      <RadioButton fx:id="petTypeRadioButton" layoutX="406.0" layoutY="24.0" mnemonicParsing="false" onAction="#typeSetPet" text="Petting zoo" toggleGroup="$typeToggleGroup" />
      <Label layoutX="39.0" layoutY="77.0" text="Length:" />
      <Label layoutX="43.0" layoutY="113.0" text="Width:" />
      <Label layoutX="40.0" layoutY="147.0" text="Height:" />
      <Label layoutX="26.0" layoutY="186.0" text="Dry Length:" />
      <Label layoutX="32.0" layoutY="221.0" text="Dry Width:" />
      <TextField fx:id="lengthTextField" layoutX="128.0" layoutY="72.0" />
      <TextField fx:id="widthTextField" layoutX="128.0" layoutY="108.0" />
      <TextField fx:id="heightTextField" layoutX="128.0" layoutY="142.0" />
      <TextField fx:id="dryLengthTextField" layoutX="128.0" layoutY="181.0" />
      <TextField fx:id="dryWidthTextField" layoutX="128.0" layoutY="216.0" />
      <Button fx:id="confirmAddPenButton" layoutX="393.0" layoutY="130.0" mnemonicParsing="false" onAction="#confirmAddPen" text="Confirm" />
   </children>
</AnchorPane>
